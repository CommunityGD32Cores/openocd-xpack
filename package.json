{
  "name": "@xpack-dev-tools/openocd",
  "version": "0.11.0-1.1",
  "description": "A binary xPack with the OpenOCD executable",
  "main": "",
  "scripts": {
    "pack": "npm pack",
    "git-log": "git log --pretty='%cd * %h %s' --date=short",
    "test": "echo \"Error: no test specified\" && exit 1",
    "liquidjs": "liquidjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xpack-dev-tools/openocd-xpack.git/"
  },
  "keywords": [
    "xpack",
    "openocd"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "https://github.com/ilg-ul/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpack-dev-tools/openocd-xpack/issues/"
  },
  "homepage": "https://github.com/xpack-dev-tools/openocd-xpack#readme",
  "xpack": {
    "binaries": {
      "destination": "./.content",
      "baseUrl": "https://github.com/xpack-dev-tools/openocd-xpack/releases/download/v0.11.0-1",
      "skip": 1,
      "platforms": {
        "darwin-x64": {
          "fileName": "xpack-openocd-0.11.0-1-darwin-x64.tar.gz",
          "sha256": "3e3719fd059d87f3433f1f6d8e37b8582e87ae6a168287eb32a85dbc0f2e1708"
        },
        "linux-arm64": {
          "fileName": "xpack-openocd-0.11.0-1-linux-arm64.tar.gz",
          "sha256": "6ffe37f305e117e21ca2b7a82b10ed06758968472aa39a2573911ae74a67da3e"
        },
        "linux-arm": {
          "fileName": "xpack-openocd-0.11.0-1-linux-arm.tar.gz",
          "sha256": "24c5de0839b8c5cb3476d6fb7b9f528daba14b434a00d60ef71d4e4da3131262"
        },
        "linux-ia32": {
          "fileName": "xpack-openocd-0.11.0-1-linux-ia32.tar.gz",
          "sha256": "fb38cc36c31eccbfcb824684596fb16dde91f9c9a42a954d9c26e678cc1a5fc1"
        },
        "linux-x64": {
          "fileName": "xpack-openocd-0.11.0-1-linux-x64.tar.gz",
          "sha256": "5972fe70a274f054503dd519b68d3909b83f017b5b8dd2b59e84b3b72c9bc3e1"
        },
        "win32-ia32": {
          "fileName": "xpack-openocd-0.11.0-1-win32-ia32.zip",
          "sha256": "b556754ee621962d41f89b229115ecf5e067b6ae76e91e210d7f53d657769296"
        },
        "win32-x64": {
          "fileName": "xpack-openocd-0.11.0-1-win32-x64.zip",
          "sha256": "ec37f535265ce93953a874b10d07e8578f9814c206d8bce6a241ba7016297e4f"
        }
      }
    },
    "bin": {
      "openocd": "./.content/bin/openocd"
    },
    "actions": {
      "generate-workflows": [
        "mkdir -p .github/workflows/",
        "cp scripts/helper/templates/body-github-pre-releases-test.md .github/workflows/",
        "cat scripts/helper/templates/build-liquid.yml | liquidjs '{ \"APP_NAME\": \"OpenOCD\", \"APP_LC_NAME\": \"openocd\" }' > .github/workflows/build.yml",
        "cat scripts/helper/templates/publish-release-liquid.yml | liquidjs '{ \"APP_NAME\": \"OpenOCD\", \"APP_LC_NAME\": \"openocd\" }' > .github/workflows/publish-release.yml",
        "cat scripts/helper/templates/test-native-liquid.yml | liquidjs '{ \"APP_NAME\": \"OpenOCD\", \"APP_LC_NAME\": \"openocd\" }' > .github/workflows/test-native.yml",
        "cat scripts/helper/templates/test-docker-linux-intel-liquid.yml | liquidjs '{ \"APP_NAME\": \"OpenOCD\" }' > .github/workflows/test-docker-linux-intel.yml",
        "cat scripts/helper/templates/test-docker-linux-arm-liquid.yml | liquidjs '{ \"APP_NAME\": \"OpenOCD\" }' > .github/workflows/test-docker-linux-arm.yml"
        ],
      "trigger-workflow-build": "bash scripts/helper/trigger-workflow-build.sh",
      "trigger-workflow-test-native": "bash scripts/helper/tests/trigger-workflow-test-native.sh",
      "trigger-workflow-test-docker-linux-intel": "bash scripts/helper/tests/trigger-workflow-test-docker-linux-intel.sh",
      "trigger-workflow-test-docker-linux-arm": "bash scripts/helper/tests/trigger-workflow-test-docker-linux-arm.sh",
      "trigger-travis-macos": "bash scripts/helper/tests/trigger-travis-macos.sh",
      "trigger-workflow-publish-release": "bash scripts/helper/trigger-workflow-publish-release.sh",
      "generate-jekyll-post": "bash scripts/helper/generate-jekyll-post.sh",
      "update-package-binaries": "bash scripts/helper/update-package-binaries.sh"
    }
  },
  "bundleDependencies": [],
  "devDependencies": {
    "liquidjs": "^9.25.1"
  }
}
